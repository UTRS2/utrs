{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8",
        "ext-json": "*",
        "deeplcom/deepl-php": "^1.6",
        "doctrine/dbal": "^3.7|^4",
        "guzzlehttp/guzzle": "^7",
        "khill/lavacharts": "^3.1",
        "kyslik/column-sortable": "^6.5",
        "laravel/framework": "^10",
        "laravel/socialite": "^5",
        "laravel/tinker": "^2",
        "league/commonmark": "^2",
        "marvinlabs/laravel-discord-logger": "^1.4",
        "proxycheck/proxycheck-php": "^0.2.2",
        "spatie/laravel-backup": "^8",
        "spatie/laravel-html": "^3.2",
        "spatie/laravel-mailcoach-mailer": "^1.0",
        "taavi/laravel-socialite-mediawiki": "^1.6",
        "taavi/laravel-torblock": "^2",
        "tio/laravel": "^1.23",
        "wikimedia/ip-utils": "^4|^5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "^2.13",
        "spatie/laravel-ignition": "^2.3",
        "fakerphp/faker": "^1.23",
        "laravel/dusk": "^7",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.9",
        "php-parallel-lint/php-console-highlighter": "^1.0.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "^10|^11",
        "taavi/faker-acceptlanguage": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "proxycheck\\": "vendor/proxycheck/proxycheck-php/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "proxycheck\\": "vendor/proxycheck/proxycheck-php/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor --exclude node_modules",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
