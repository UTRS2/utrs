{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "addwiki/mediawiki-api": "^2.8",
        "addwiki/mediawiki-api-base": "^2.8",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^8.15",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/socialite": "^5.1",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "league/commonmark": "^1.3",
        "spatie/laravel-backup": "^6.10",
        "taavi/laravel-socialite-mediawiki": "^1.3",
        "taavi/laravel-torblock": "^1.1",
        "tio/laravel": "^1.18",
        "wikimedia/ip-utils": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.7",
        "facade/ignition": "^2.5.2",
        "fakerphp/faker": "^1.12",
        "laravel/dusk": "^6.8.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.1.0",
        "php-parallel-lint/php-console-highlighter": "^0.5.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpunit/phpunit": "^9.3",
        "taavi/faker-acceptlanguage": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
   },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor --exclude node_modules",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
